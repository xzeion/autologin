{
    "manifest_version": 2, 

    "name": "Signal Auto-Login",
    "description": "This plugin will keep you logged into signal",
    "version": "1.0",
	"options_page": "options.html",
    
    "browser_action": {
        "default_icon": "signal_on.png",
        "default_popup": "popup.html"
    },
    "permissions":[
        "activeTab"
    ]
}
