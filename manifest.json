{
    "manifest_version": 1,

    "name": "Signal Auto-Login"
    "description": "This plugin will keep you logged into signal"
    "version": "1.0",
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions":[
        "activeTab"
    ]
}
